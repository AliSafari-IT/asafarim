  "name": "asafarim-ui",
  "version": "1.2.2",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.9.0",
  "overrides": {
    "postcss-nesting": "^13.0.1"
  },
  "repository": "https://github.com/AliSafari-IT/asafarim.git",
  "scripts": {
    "co": "cd ../.. && yarn co",
    "preinstall": "npx only-allow yarn",
    "api": "node ./server.js",
    "serve": "cross-env NODE_ENV=development concurrently \"postcss ./src/index.css -o public/build/tailwind.css --watch\"  \"live-server ./\"",
    "dev": "concurrently \"yarn workspace asafarim-api start\" \"yarn workspace asafarim-ui start\"",
    "rebuild": "yarn install && vite build",
    "clean": "rm -rf node_modules .yarn yarn.lock && yarn cache clean --all",
    "reinstall": "echo \" üì¶ Clearing cache and installing dependencies...\" && yarn clean && yarn install",
    "start": "vite --port 3000",
    "build:prod": "yarn reinstall && tsc --noEmit && echo \" ‚öôÔ∏è  Starting production build...\" && vite build",
    "build:dev": "yarn install --check-files && tsc --noEmit && vite build",
    "build": "cross-env NODE_ENV=production npx vite build",
    "build:npx": "cross-env NODE_ENV=production npx vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "stop": "npx kill-port 3000",
    "rs": "yarn stop &&  yarn dev",
    "test": "vitest",
    "test:run": "vitest run"
  },
