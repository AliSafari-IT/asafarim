{
  "name": "asafarim-cli",
  "version": "1.1.0",
  "description": "A simple terminal interface using Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "clean": "rm -rf node_modules .pnpm pnpm-lock.yaml",
    "reinstall": "echo \" ðŸ“¦ Clearing cache and installing dependencies...\" && pnpm store prune && pnpm clean && pnpm install && pnpm approve-builds",
    "rebuild": "node-gyp rebuild",
    "build:prod": "pnpm install --production && node-gyp rebuild",
    "build:dev": "pnpm install && node-gyp rebuild"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.1.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "dotenv": "^16.5.0"
  }
}
