{
  "name": "asafarim-ui",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "server": {
    "proxy": {
      "/api": "http://localhost:5000"
    }
  },
  "scripts": {
    "api": "node ./server.js",
    "serve": "cross-env NODE_ENV=development concurrently \"postcss ./src/index.css -o public/build/tailwind.css --watch\"  \"live-server ./\"",
    "dev": "concurrently \"yarn workspace asafarim-api start\" \"yarn workspace asafarim-ui start\"",
    "rebuild": "yarn install && vite build",
    "clean": "node -e \"const { rmSync } = require('fs'); const paths = ['./node_modules', './package-lock.json']; paths.forEach(p => { try { rmSync(p, { recursive: true, force: true }); console.log('Removed:', p); } catch (err) { if (err.code !== 'ENOENT') console.error('Error removing', p, err); } });\"",
    "reinstall": "yarn clean && yarn install --immutable --immutable-cache",
    "start": "vite",
    "build:prod": "yarn install --immutable --immutable-cache && tsc -b && vite build",
    "build:dev": "yarn install && tsc -b && vite build",
    "build": "cross-env NODE_ENV=production yarn build:prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "stop": "npx kill-port 3000",
    "rs": "yarn stop && yarn dev",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fluentui-contrib/azure-theme": "^0.0.3",
    "@fluentui-contrib/react-resize-handle": "^0.6.1",
    "@fluentui-contrib/react-shadow": "^0.1.0",
    "@fluentui-contrib/react-tree-grid": "^0.1.3",
    "@fluentui/font-icons-mdl2": "^8.5.54",
    "@fluentui/react": "^8.121.4",
    "@fluentui/react-components": "^9.55.1",
    "@fluentui/react-icons": "^2.0.266",
    "@fluentui/react-nav-preview": "^0.10.3",
    "@fluentui/react-tree": "^9.8.8",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "@types/lodash": "^4.17.14",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "create-react-class": "^15.7.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "d3": "^7.9.0",
    "d3-cloud": "^1.2.7",
    "docx": "^9.0.2",
    "file-saver": "^2.0.5",
    "html-to-image": "^1.11.11",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "jwt-decode": "^4.0.0",
    "live-server": "^1.2.2",
    "loadash": "^1.0.0",
    "marked": "^14.1.3",
    "postcss": "^8.4.47",
    "react": "^18.2.0",
    "react-d3-treemap": "^2.0.6",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.0.1",
