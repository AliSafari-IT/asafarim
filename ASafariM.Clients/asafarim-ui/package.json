{
  "name": "asafarim-ui",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "server": {
    "proxy": {
      "/api": "http://localhost:5000"
    }
  },
  "scripts": {
    "api": "node ./server.js",
    "serve": "cross-env NODE_ENV=development concurrently \"postcss ./src/index.css -o public/build/tailwind.css --watch\"  \"live-server ./\"",
    "dev": "concurrently \"yarn workspace asafarim-api start\" \"yarn workspace asafarim-ui start\"",
    "rebuild": "yarn install && vite build",
    "preinstall": "npx kill-port 3000",
    "install": "yarn",
    "reinstall": "rm -rf node_modules && rm -rf dist && rm -rf .yarn/cache && yarn run preinstall && yarn run install",
    "start": "vite",
    "build": "yarn && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "stop": "npx kill-port 3000",
    "rs": "yarn stop && yarn dev",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tailwindcss/forms": "^0.5.9",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.4",
    "@types/create-react-class": "^15",
    "@types/d3": "^7.4.3",
    "@types/d3-cloud": "^1",
    "@types/file-saver": "^2.0.7",
    "@types/graceful-fs": "^4.1.9",
    "@types/jest": "^29.5.14",
    "@types/live-server": "^1",
    "@types/node": "^22.12.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-test-renderer": "^19",
    "@types/tinycolor2": "^1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "file-loader": "^6.2.0",
    "globals": "^15.9.0",
    "https": "^1.0.0",
    "jsdom": "^26.0.0",
    "postcss-cli": "^11.0.0",
    "react-test-renderer": "^19.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "remark-stringify": "^11.0.0",
    "sass": "^1.85.0",
    "sass-embedded": "^1.79.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^4.5.0",
    "vitest": "^3.0.4",
    "webpack": "^5.90.1"
  }
}
